<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="470.0" prefWidth="689.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controller.LogInController">
    <TextField fx:id="usernameTextField" layoutX="284.0" layoutY="218.0" />
    <Label fx:id="titleLabel" alignment="CENTER" layoutX="108.0" layoutY="98.0" prefHeight="42.0" prefWidth="449.0" text="Scheduling Assistant">
        <font>
          <Font name="Book Antiqua Bold" size="30.0" />
        </font>
    </Label>
    <Label fx:id="usernameLabel" alignment="CENTER" layoutX="175.0" layoutY="222.0" prefHeight="17.0" prefWidth="109.0" text="Username" />
    <TextField fx:id="passwordTextField" layoutX="284.0" layoutY="267.0" />
    <Label fx:id="passwordLabel" alignment="CENTER" layoutX="177.0" layoutY="271.0" prefHeight="17.0" prefWidth="109.0" text="Password" />
    <Button fx:id="logInButton" layoutX="284.0" layoutY="318.0" mnemonicParsing="false" onAction="#onActionLogIn" text="Log In" />
    <Button fx:id="exitButton" layoutX="398.0" layoutY="318.0" mnemonicParsing="false" onAction="#onActionExit" text="Exit" />
    <Label fx:id="welcomeLabel" alignment="CENTER" layoutX="224.0" layoutY="161.0" prefHeight="42.0" prefWidth="218.0" text="Welcome! Please log in." />
   <Label fx:id="locationLabel" alignment="CENTER" layoutX="187.0" layoutY="404.0" prefHeight="17.0" prefWidth="77.0" text="Location:" />
   <Label fx:id="locationValueLabel" alignment="CENTER" layoutX="266.0" layoutY="404.0" prefHeight="17.0" prefWidth="158.0" text="loc" />
</AnchorPane>
