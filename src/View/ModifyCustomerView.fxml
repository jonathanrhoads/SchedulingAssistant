<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>


<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="Controller.ModifyCustomerController">
   <Label layoutX="242.0" layoutY="30.0" text="Modify Customer">
      <font>
         <Font name="System Bold" size="14.0"/>
      </font>
   </Label>
   <Label layoutX="83.0" layoutY="200.0" text="Country"/>
   <Label layoutX="83.0" layoutY="237.0" text="Division"/>
   <Label layoutX="60.0" layoutY="72.0" text="Customer ID:"/>
   <ComboBox fx:id="countryComboBox" layoutX="141.0" layoutY="196.0" onAction="#onActionCountryComboBox"
             prefHeight="26.0" prefWidth="200.0"/>
   <ComboBox fx:id="divisionComboBox" layoutX="141.0" layoutY="233.0" prefHeight="26.0" prefWidth="200.0"/>
   <TextField fx:id="customerIdTextField" disable="true" editable="false" layoutX="143.0" layoutY="67.0"
              prefHeight="26.0" prefWidth="42.0"/>
   <Label layoutX="95.0" layoutY="114.0" text="Name"/>
   <Label layoutX="85.0" layoutY="155.0" text="Address"/>
   <Label layoutX="86.0" layoutY="346.0" text="Phone"/>
   <TextField fx:id="nameTextField" layoutX="143.0" layoutY="109.0" prefHeight="26.0" prefWidth="200.0"/>
   <TextField fx:id="addressTextField" layoutX="143.0" layoutY="151.0" prefHeight="26.0" prefWidth="200.0"/>
   <TextField fx:id="phoneTextField" layoutX="141.0" layoutY="341.0" prefHeight="26.0" prefWidth="200.0"/>
   <Label layoutX="63.0" layoutY="280.0" text="Postal Code"/>
   <TextField fx:id="postalCodeTextField" layoutX="141.0" layoutY="276.0" prefHeight="26.0" prefWidth="200.0"/>
   <Button fx:id="saveButton" layoutX="429.0" layoutY="315.0" mnemonicParsing="false" onAction="#onActionSave"
           text="Save"/>
   <Button fx:id="cancelButton" layoutX="486.0" layoutY="315.0" mnemonicParsing="false" onAction="#onActionCancel"
           text="Cancel"/>
</AnchorPane>
